<health-rules>
        <health-rule>
            <name>Elasticsearch Nodes | Drive D: disk space low {hrname}</name>
            <type>INFRASTRUCTURE</type>
            <description/>
            <enabled>true</enabled>
            <is-default>true</is-default>
            <always-enabled>true</always-enabled>
            <duration-min>10</duration-min>
            <wait-time-min>60</wait-time-min>
            <affected-entities-match-criteria>
                <affected-infra-match-criteria>
                    <type>NODES</type>
                    <node-match-criteria>
                        <type>NODES_OF_SPECIFC_TIERS</type>
                        <node-meta-info-match-criteria/>
                        <vm-sys-properties/>
                        <env-properties/>
                    </node-match-criteria>
                    <components>
                        <application-component>{tier}</application-component>
                    </components>
                </affected-infra-match-criteria>
            </affected-entities-match-criteria>
            <critical-execution-criteria>
                <entity-aggregation-scope>
                    <type>ANY</type>
                    <value>0</value>
                </entity-aggregation-scope>
                <policy-condition>
                    <type>leaf</type>
                    <display-name>condition 1</display-name>
                    <condition-value-type>ABSOLUTE</condition-value-type>
                    <condition-value>10.0</condition-value>
                    <operator>LESS_THAN</operator>
                    <condition-expression>({y}/({x}+{y}))*100</condition-expression>
                    <use-active-baseline>false</use-active-baseline>
                    <metric-expression>
                        <type>boolean</type>
                        <operator>MULTIPLY</operator>
                        <expression1>
                            <type>boolean</type>
                            <operator>DIVIDE</operator>
                            <expression1>
                                <type>leaf</type>
                                <function-type>VALUE</function-type>
                                <value>0</value>
                                <is-literal-expression>false</is-literal-expression>
                                <display-name>y</display-name>
                                <metric-definition>
                                    <type>LOGICAL_METRIC</type>
                                    <logical-metric-name>Hardware Resources|Disks|d-|Space Available</logical-metric-name>
                                </metric-definition>
                            </expression1>
                            <expression2>
                                <type>boolean</type>
                                <operator>PLUS</operator>
                                <expression1>
                                    <type>leaf</type>
                                    <function-type>VALUE</function-type>
                                    <value>0</value>
                                    <is-literal-expression>false</is-literal-expression>
                                    <display-name>x</display-name>
                                    <metric-definition>
                                    <type>LOGICAL_METRIC</type>
                                    <logical-metric-name>Hardware Resources|Disks|d-|Space Used</logical-metric-name>
                                    </metric-definition>
                                </expression1>
                                <expression2>
                                    <type>leaf</type>
                                    <function-type>VALUE</function-type>
                                    <value>0</value>
                                    <is-literal-expression>false</is-literal-expression>
                                    <display-name>y</display-name>
                                    <metric-definition>
                                    <type>LOGICAL_METRIC</type>
                                    <logical-metric-name>Hardware Resources|Disks|d-|Space Available</logical-metric-name>
                                    </metric-definition>
                                </expression2>
                            </expression2>
                        </expression1>
                        <expression2>
                            <type>leaf</type>
                            <value>100</value>
